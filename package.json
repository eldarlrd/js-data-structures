{
  "name": "js-data-structures",
  "private": true,
  "version": "1.2.2",
  "type": "module",
  "keywords": [
    "data-structures",
    "javascript"
  ],
  "description": "Manual and Explorable JS Data Structures",
  "repository": "github:eldarlrd/js-data-structures",
  "author": "Eldar Pashazade <eldarlrd@pm.me> (https://eldarlrd.github.io)",
  "license": "GPL-3.0-only",
  "main": "src/main.js",
  "scripts": {
    "start": "node src/main.js",
    "test": "jest --watchAll",
    "cover": "jest --coverage",
    "lint": "eslint src __tests__",
    "format": "prettier '{src,__{tests,mocks,snapshots}__}/**/*.{js,jsx}' --write",
    "fix": "npm run lint -- --fix && npm run format"
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true,
    "bracketSameLine": true,
    "trailingComma": "none",
    "arrowParens": "avoid",
    "experimentalTernaries": true
  },
  "dependencies": {
    "cli-color": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@eslint/js": "^9.39.2",
    "babel-jest": "^30.2.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.11.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "prettier": "^3.7.4"
  },
  "engines": {
    "node": ">=24.12.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
