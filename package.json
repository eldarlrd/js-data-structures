{
  "name": "js-data-structures",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "keywords": [
    "data-structures",
    "javascript"
  ],
  "description": "Manual and Explorable JavaScript Data Structures",
  "repository": "github:eldarlrd/js-data-structures",
  "author": "Eldar Pashazade <eldarlrd@pm.me> (https://eldarlrd.github.io)",
  "license": "GPL-3.0-only",
  "scripts": {
    "lint": "eslint src --ext .js,.jsx --report-unused-disable-directives",
    "pretty": "prettier '{src,__{tests,mocks,snapshots}__}/**/*.{js,jsx}' --write",
    "format": "pnpm lint --fix && pnpm pretty"
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true,
    "bracketSameLine": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.1.0"
  }
}
